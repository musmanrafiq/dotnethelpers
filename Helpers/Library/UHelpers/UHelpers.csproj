<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>UHelpers</PackageId>
    <Title>UHelpers, Common c# Helpers</Title>
    <Version>1.0.0</Version>
    <Authors>M.Usman Rafiq</Authors>
    <Company>M.Usman Rafiq</Company>
    <Description>Common c# helper methods that are used in every project.</Description>
    <RepositoryUrl>https://github.com/musmanrafiq/dotnethelpers</RepositoryUrl>
    <PackageTags>c# helpers, common c# helper methods</PackageTags>
    <PackageReleaseNotes>Started base for common c# helper methods</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <SignAssembly>false</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

</Project>
